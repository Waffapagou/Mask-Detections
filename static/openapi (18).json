{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "Mask API",
    "description": "An API which return a prediction of person if this person wear or not a mask"
  },
  "paths": {
    "/uploadfile": {
      "post": {
        "tags": [
          "MEDIA"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "media": {
                    "type": "string",
                    "format": "base64"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "ok",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}